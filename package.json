{
    "name": "kazagumo-playermoved",
    "version": "1.0.0",
    "description": "A modified version of the PlayersMoved plugin, built with support for Oceanic.JS",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint 'src/*.{ts,js}'",
        "build:full": "npm run lint && npm run format && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/omfori/kazagumo-playermoved.git"
    },
    "keywords": [
        "kazagumo",
        "module",
        "kazagumo-plugin",
        "kazagumo-playermoved"
    ],
    "author": "jedddg",
    "license": "AGPL-3.0-only",
    "bugs": {
        "url": "https://github.com/omfori/kazagumo-playermoved/issues"
    },
    "homepage": "https://github.com/omfori/kazagumo-playermoved#readme",
    "dependencies": {
        "kazagumo": "^2.3.2",
        "oceanic.js": "^1.4.1"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "prettier": "^2.8.3",
        "typescript": "^4.9.4"
    }
}
